{
  "singleQuote": true,
  "printWidth": 100,
  "tabWidth": 2,
  "semi": true,
  "trailingComma": "es5",
  "requirePragma": false,
  "arrowParens": "always",
  "endOfLine": "auto",
  "importOrder": [
    "^react(.*)",
    "^@/pages/(.*)$",
    "^@/components/(.*)$",
    "^@/apis/(.*)$",
    "^@/hooks/(.*)$",
    "^@/routes/(.*)$",
    "^@/type/(.*)$",
    "^@/utils/(.*)$",
    "^@/assets/(.*)$",
    "^@/styles/(.*)$",
    "^@/(.*)$",
    "^[./]"
  ]
}
